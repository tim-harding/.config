{
  "arrow": {
    "body": "($1) => {$2}",
    "prefix": "arrow"
  },
  "component": {
    "body": [
      "<script setup lang=\"ts\">$1</script>",
      "",
      "<template>$2</template>",
      "",
      "<style module=\"s\">$3</style>"
    ],
    "prefix": "component"
  },
  "for": {
    "body": [
      "for ($1) {",
      "  $2",
      "}"
    ],
    "prefix": "for"
  },
  "function": {
    "body": [
      "function $1($2)$3 {",
      "  $4",
      "}"
    ],
    "prefix": "function"
  },
  "if": {
    "body": [
      "if ($1) {",
      "  $2",
      "}"
    ],
    "prefix": "if"
  },
  "interface": {
    "body": [
      "interface $1 {",
      "  $2",
      "}"
    ],
    "prefix": "interface"
  }
}
