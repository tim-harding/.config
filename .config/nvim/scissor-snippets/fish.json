{
  "begin": {
    "body": [
      "begin",
      "    $1",
      "end"
    ],
    "prefix": "begin"
  },
  "elif": {
    "body": [
      "else if ${1:cond}",
      "    $2"
    ],
    "prefix": "elif"
  },
  "else": {
    "body": [
      "else",
      "    $2"
    ],
    "prefix": "else"
  },
  "for": {
    "body": [
      "for $1 in $2",
      "    $3",
      "end"
    ],
    "prefix": "for"
  },
  "function": {
    "body": [
      "function ${1:name}",
      "    $2",
      "end"
    ],
    "prefix": "function"
  },
  "if": {
    "body": [
      "if ${1:cond}",
      "    $2",
      "end"
    ],
    "prefix": "if"
  },
  "switch + match": {
    "body": [
      "switch ${1:value}",
      "case ${2:glob}",
      "    $3",
      "end"
    ],
    "prefix": [
      "switch",
      "match"
    ]
  },
  "while": {
    "body": [
      "while $1",
      "    $2",
      "end"
    ],
    "prefix": "while"
  }
}
