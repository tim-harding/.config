{
  "/*": {
    "body": [
      "/*",
      " * $1",
      " */"
    ],
    "prefix": "doc"
  },
  "arrow": {
    "body": "($1) => {$2}",
    "prefix": "ar"
  },
  "arrow-1": {
    "body": "($1) => {$2}",
    "prefix": "arrow"
  },
  "async_function": {
    "body": [
      "async function $1($2)$3 {",
      "  $4",
      "}"
    ],
    "prefix": "async_function"
  },
  "elif": {
    "body": [
      "else if ($1) {",
      "  $2",
      "}"
    ],
    "prefix": "elif"
  },
  "else": {
    "body": [
      "else {",
      "  $1",
      "}"
    ],
    "prefix": "else"
  },
  "for": {
    "body": [
      "for ($1) {",
      "  $2",
      "}"
    ],
    "prefix": "for"
  },
  "fori": {
    "body": [
      "for (let $1 = 0; $1 < $2; $1++) {",
      "  $3",
      "}"
    ],
    "prefix": "fori"
  },
  "function": {
    "body": [
      "function $1($2)$3 {",
      "  $4",
      "}"
    ],
    "prefix": "fn"
  },
  "if": {
    "body": [
      "if ($1) {",
      "  $2",
      "}"
    ],
    "prefix": "if"
  },
  "modimport": {
    "body": "import * as $1 from \"./$1\";",
    "prefix": "modimport"
  },
  "switch": {
    "body": [
      "switch ($1) {",
      "  $2",
      "}"
    ],
    "prefix": "switch"
  },
  "test": {
    "body": "test('$1', () => {$2})",
    "prefix": "test"
  },
  "test-imports": {
    "body": "import { expect, test } from \"vitest\";",
    "prefix": "test-imports"
  },
  "try": {
    "body": [
      "try {",
      "  $1",
      "} catch (e) {",
      "  $2",
      "}"
    ],
    "prefix": "try"
  },
  "while": {
    "body": [
      "while ($1) {",
      "  $2",
      "}"
    ],
    "prefix": "while"
  }
}
