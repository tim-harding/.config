{
  "/*": {
    "body": [
      "/**",
      " * $1",
      " */"
    ],
    "prefix": "/*"
  },
  "callback_doc": {
    "body": "@callback ${1:Name}",
    "prefix": "callback_doc"
  },
  "link_doc": {
    "body": "{@link ${1:type}}",
    "prefix": "link_doc"
  },
  "param_doc": {
    "body": "@param {${1:Type}} ${2:name}",
    "prefix": "param_doc"
  },
  "property_doc": {
    "body": "@property {${1:Type}} ${2:name}",
    "prefix": "property_doc"
  },
  "return_doc": {
    "body": "@return {${1:Type}}",
    "prefix": "return_doc"
  },
  "throws_doc": {
    "body": "@throws {${1:Type}}",
    "prefix": "throws_doc"
  },
  "type_doc": {
    "body": "/** @type {${1:Type}} */",
    "prefix": "type_doc"
  },
  "typedef_doc": {
    "body": "@typedef {${1:Type}} ${2:Name}",
    "prefix": "typedef_doc"
  },
  "web_component": {
    "body": [
      "class $1 extends HTMLElement {",
      "  static observedAttributes = [];",
      "",
      "  constructor() {",
      "    super();",
      "  }",
      "",
      "  connectedCallback() {}",
      "  disconnectedCallback() {}",
      "  adoptedCallback() {}",
      "  attributeChangedCallback(name, oldValue, newValue) {}",
      "}",
      "",
      "customElements.define(\"$2\", $1);"
    ],
    "prefix": "web_component"
  }
}
